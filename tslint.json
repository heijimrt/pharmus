{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [ 80 ]
    },
    "no-console": [ false ],
    "quotemark": [ true, "single" ],
    "semicolon": [ true, "never" ],
    "curly": false,
    "arrow-parens": false,
  },
  "rulesDirectory": [],
  "object-literal-sort-keys": false
}
